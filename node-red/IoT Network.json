        "func": "//variables\n\nTx = global.get('Tx');\nconst privateKey = \"8f2a55949038a9610f50fb23b5883af3b4ecb3c3bb792cbcefbd1542c692be63\";\nconst Provider = new PrivateKeyProvider(privateKey, \"http://127.0.0.1:8545\");\nconst w3 = new Web3(new Web3.providers.HttpProvider('http://127.0.0.1:8545'));\nw3.setProvider(Provider);\n//Contract.setProvider(Provider);\n\nvar abi = [\n\t{\n\t\t\"inputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"id\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t},\n\t\t\t{\n\t\t\t\t\"internalType\": \"string\",\n\t\t\t\t\"name\": \"time\",\n\t\t\t\t\"type\": \"string\"\n\t\t\t},\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"value\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t}\n\t\t],\n\t\t\"name\": \"addData\",\n\t\t\"outputs\": [],\n\t\t\"stateMutability\": \"payable\",\n\t\t\"type\": \"function\"\n\t},\n\t{\n\t\t\"inputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"uint256\",\n\t\t\t\t\"name\": \"\",\n\t\t\t\t\"type\": \"uint256\"\n\t\t\t}\n\t\t],\n\t\t\"name\": \"dataSet\",\n\t\t\"outputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"sensorId\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t},\n\t\t\t{\n\t\t\t\t\"internalType\": \"string\",\n\t\t\t\t\"name\": \"timeStamp\",\n\t\t\t\t\"type\": \"string\"\n\t\t\t},\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"value\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t}\n\t\t],\n\t\t\"stateMutability\": \"view\",\n\t\t\"type\": \"function\"\n\t},\n\t{\n\t\t\"inputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"givenId\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t}\n\t\t],\n\t\t\"name\": \"getAllDataBySensor\",\n\t\t\"outputs\": [\n\t\t\t{\n\t\t\t\t\"components\": [\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\t\t\"name\": \"sensorId\",\n\t\t\t\t\t\t\"type\": \"int256\"\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"string\",\n\t\t\t\t\t\t\"name\": \"timeStamp\",\n\t\t\t\t\t\t\"type\": \"string\"\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\t\t\"name\": \"value\",\n\t\t\t\t\t\t\"type\": \"int256\"\n\t\t\t\t\t}\n\t\t\t\t],\n\t\t\t\t\"internalType\": \"struct DataStorage.Data[]\",\n\t\t\t\t\"name\": \"returnData\",\n\t\t\t\t\"type\": \"tuple[]\"\n\t\t\t}\n\t\t],\n\t\t\"stateMutability\": \"view\",\n\t\t\"type\": \"function\"\n\t},\n\t{\n\t\t\"inputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"uint256\",\n\t\t\t\t\"name\": \"arrayPointer\",\n\t\t\t\t\"type\": \"uint256\"\n\t\t\t}\n\t\t],\n\t\t\"name\": \"getData\",\n\t\t\"outputs\": [\n\t\t\t{\n\t\t\t\t\"components\": [\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\t\t\"name\": \"sensorId\",\n\t\t\t\t\t\t\"type\": \"int256\"\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"string\",\n\t\t\t\t\t\t\"name\": \"timeStamp\",\n\t\t\t\t\t\t\"type\": \"string\"\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\t\t\"name\": \"value\",\n\t\t\t\t\t\t\"type\": \"int256\"\n\t\t\t\t\t}\n\t\t\t\t],\n\t\t\t\t\"internalType\": \"struct DataStorage.Data\",\n\t\t\t\t\"name\": \"returnData\",\n\t\t\t\t\"type\": \"tuple\"\n\t\t\t}\n\t\t],\n\t\t\"stateMutability\": \"view\",\n\t\t\"type\": \"function\"\n\t}\n];\n\nconst account = '0xfe3b557e8fb62b89f4916b721be55ceb828dbd73';\nconst contract_address = '0x9a3DBCa554e9f6b9257aAa24010DA8377C57c17e';\n//const gasPrice = w3.eth.getGasPrice();\n//const Nonce = Number(w3.eth.getTransactionCount(account));\nvar contract = new Contract(abi, contract_address, Provider);\n//const gasEstimate = contract.methods.addData(msg.sensorId, msg.date, msg.Value).estimateGas();\n\nvar trans = {\n'from': account,\n'to': contract_address,\n'gasPrice': 0,\n'gas': 100000,\n//'nonce': Nonce+1,\n'value': 1,\n'chainId': w3.eth.net.getId,\n'data': contract.methods.addData(msg.sensorId, msg.date, msg.Value).encodeABI(),\n}\n\nconst signPromise = w3.eth.signTransaction(trans, trans.from);\n\nsignPromise.then((signedTx) => {\n  const sentTx = w3.eth.sendSignedTransaction(signedTx.raw || signedTx.rawTransaction);  sentTx.on(\"receipt\", receipt => {\n  });\n  sentTx.on(\"error\", err => {\n  });\n}).catch((err) => {\n  console.error(err);\n});",
