pe\": \"string\"\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\t\t\"name\": \"value\",\n\t\t\t\t\t\t\"type\": \"int256\"\n\t\t\t\t\t}\n\t\t\t\t],\n\t\t\t\t\"internalType\": \"struct DataStorage.Data[]\",\n\t\t\t\t\"name\": \"returnData\",\n\t\t\t\t\"type\": \"tuple[]\"\n\t\t\t}\n\t\t],\n\t\t\"stateMutability\": \"view\",\n\t\t\"type\": \"function\"\n\t},\n\t{\n\t\t\"inputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"uint256\",\n\t\t\t\t\"name\": \"arrayPointer\",\n\t\t\t\t\"type\": \"uint256\"\n\t\t\t}\n\t\t],\n\t\t\"name\": \"getData\",\n\t\t\"outputs\": [\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"Id\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t},\n\t\t\t{\n\t\t\t\t\"internalType\": \"string\",\n\t\t\t\t\"name\": \"time\",\n\t\t\t\t\"type\": \"string\"\n\t\t\t},\n\t\t\t{\n\t\t\t\t\"internalType\": \"int256\",\n\t\t\t\t\"name\": \"sensorvalue\",\n\t\t\t\t\"type\": \"int256\"\n\t\t\t}\n\t\t],\n\t\t\"stateMutability\": \"view\",\n\t\t\"type\": \"function\"\n\t}\n];\n\nvar contract = new ethers.Contract(address,abi,wallet);\nvar sendPromise = contract.addData(msg.deviceId, msg.date, msg.Value);\n\n/*\nsendPromise.then(function(transaction){\n  console.log(transaction);\n});\n*/\n\nasync function main() {\nconst tx = await contract.addData(msg.deviceId, msg.date, msg.Value);\nawait tx.wait();\n}\n\nmain();",
